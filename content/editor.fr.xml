<?xml version="1.0" encoding="iso-8859-1"?>
<editor>
	<tools>
		<item id="save">Enregistrer</item>
		<item id="compile">Compiler</item>
		<item id="execute">Éxécuter</item>
		<item id="options">Options</item>
	</tools>
	<messages>
		<msg id="fileOpenError">Erreur lors du chargement du fichier: [fileOpenError]</msg>
		<msg id="fileNotFoundError">Fichier introuvable: [fileNotFoundError]</msg>
		<msg id="fileSaveOK">Fichier enregistré avec succès: [fileSaveOK]</msg>		
	</messages>
	<files>
		<item extensions="java|" type="java" actions="save|execute|compile">Java</item>
		<item extensions="html|htm|shl|xml|" type="html" actions="save">HTML/XML</item>
		<item extensions="php|" type="php" actions="save">PHP</item>		
		<item extensions="txt|bat|sh|nfo|" type="text" actions="save">Texte</item>
		<item extensions="class|exe|gif|jpg|jpeg|mov|png|doc|pdf|mp3|swf|fla|mpg|mpeg|qt|avi|" type="binary" actions="">Fichier binaire</item>
	</files>
</editor>