<?xml version="1.0" encoding="iso-8859-1"?>
<editor>
	<tools>
		<i id="save">Enregistrer</i>
		<i id="view">XXXXXX</i>		
		<i id="compile">Compiler</i>
		<i id="execute">Éxécuter</i>
	</tools>
	<messages>
		<i id="fileOpenError">Erreur lors du chargement du fichier: [fileOpenError]</i>
		<i id="fileNotFoundError">Fichier introuvable: [fileNotFoundError]</i>
		<i id="fileSaveOK">Fichier enregistré avec succès: [fileSaveOK]</i>		
	</messages>
	<files>
		<i extensions="java|" type="java" actions="save|execute|compile|">Java</i>
		<i extensions="html|htm|shl|xml|" type="html" actions="save|view|">HTML/XML</i>
		<i extensions="css|" type="css" actions="save|">CSS</i>
		<i extensions="js|" type="javascript" actions="save|">JavaScript</i>
		<i extensions="pl|" type="perl" actions="save|execute|">PERL</i>
		<i extensions="py|" type="python" actions="save|execute|">Python</i>
		<i extensions="c|" type="c" actions="save|compile|execute|">C</i>
		<i extensions="cpp|" type="cpp" actions="save|compile|execute|">C++</i>
		<i extensions="rb|" type="ruby" actions="save|execute|">Ruby</i>
		<i extensions="php|" type="php" actions="save|view|">PHP</i>
		<i extensions="txt|bat|sh|nfo|" type="text" actions="save">Texte</i>
		<i extensions="class|exe|gif|jpg|jpeg|mov|png|doc|pdf|mp3|swf|fla|mpg|mpeg|qt|avi|" type="binary" actions="">Fichier binaire</i>
	</files>
</editor>