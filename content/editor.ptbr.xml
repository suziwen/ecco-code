<editor>

	<tools>
		<item id="save">Salvar</item>
		<item id="compile">Compilar</item>
		<item id="execute">Executar</item>
		<item id="options">Op&amp;ccedil;&amp;otilde;es</item>
	</tools>

	<messages>
		<msg id="fileOpenError">Erro ao abrir arquivo: [fileOpenError]</msg>
		<msg id="fileNotFoundError">Arquivo nao encontrado: [fileNotFoundError]</msg>
		<msg id="fileSaveOK">Arquivo salvo: [fileSaveOK]</msg>		
	</messages>
	
	<filetypes>
		<item extensions="java|" openas="java" actions="save|execute|compile">Java</item>
		<item extensions="html|htm|shl|xml|" openas="html" actions="save">HTML/XML</item>
		<item extensions="php|" openas="php" actions="save">PHP</item>		
		<item extensions="txt|bat|sh|nfo|" openas="text" actions="save">Texto</item>
		<item extensions="exe|gif|jpg|jpeg|mov|png|doc|pdf|mp3|swf|fla|mpg|mpeg|qt|avi|" openas="binary" actions="">Binary</item>
	</filetypes>
	
</editor>