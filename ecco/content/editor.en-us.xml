<?xml version="1.0" encoding="iso-8859-1"?>
<editor>
	<tools>
		<item id="save">Save</item>
		<item id="compile">Compile</item>
		<item id="execute">Run</item>
		<item id="options">Options</item>
	</tools>
	<messages>
		<msg id="fileOpenError">Error while loading file: [fileOpenError]</msg>
		<msg id="fileNotFoundError">File not found: [fileNotFoundError]</msg>
		<msg id="fileSaveOK">File saved: [fileSaveOK]</msg>		
	</messages>
	<files>
		<item extensions="java|" type="java" actions="save|execute|compile">Java</item>
		<item extensions="html|htm|shl|xml|" type="html" actions="save">HTML/XML</item>
		<item extensions="php|" type="php" actions="save">PHP</item>		
		<item extensions="txt|bat|sh|nfo|" type="text" actions="save">Plain text</item>
		<item extensions="class|exe|gif|jpg|jpeg|mov|png|doc|pdf|mp3|swf|fla|mpg|mpeg|qt|avi|" type="binary" actions="">Binary</item>
	</files>
</editor>